{% extends 'base.html.twig' %}

{% block title %}
users!
{% endblock %}

{% block body %}

  <h1 class="text-center m-5">{{ title }}</h1>

<div class="text-center">
  <h2 class="m-5">Users ({{ users|length }})</h2>
  <ul class="list-group">
    {% for user in users %}
      <li class="list-group-item">
        <h5>{{ user[0] }}</h5>
        <p><small>id: </small>{{ user[1] }}</p>
        <p><small>occupation: </small>{{ user[2] }}</p>
      </li>
    {% endfor %}
  </ul>
</div>

{% endblock %}
